<h1>Log In</h1>
<div class="main">
	<p>Try it out! <br> Username: 'admin' <br> Password: 'password'</p>
	<hr>
<%= form_for(:session, url: login_path) do |f| %>

	<%= f.label :username %>
	<%= f.text_field :username, class: 'form-control' %>

	<%= f.label :password %>
	<%= f.password_field :password, class: 'form-control' %>

	<%= f.submit 'Log In', class: 'btn' %>
<% end %>
</div>